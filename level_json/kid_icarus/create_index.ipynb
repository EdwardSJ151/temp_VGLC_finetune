   "source": [
    "import json\n",
    "\n",
    "with open('kid_icarus.json', 'r') as f:\n",
    "    level_prompts = json.load(f)\n",
    "\n",
    "for i, entry in enumerate(level_prompts):\n",
    "    entry['index'] = i\n",
    "\n",
    "with open('kid_icarus_indexed.json', 'w') as f:\n",
    "    json.dump(level_prompts, f, indent=4)"
   ]
  }
